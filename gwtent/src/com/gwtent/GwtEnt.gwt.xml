<module>
    <inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.http.HTTP"/>
    <inherits name="com.google.gwt.xml.XML"/>
    
    <!-- <inherits name="com.gwtext.GwtExt"/> -->
    
    <generate-with class="com.gwtent.gen.reflection.ReflectionProxyGenerator">
		<when-type-assignable class="com.gwtent.client.reflection.Reflection"/>
	</generate-with>
	
	<define-property name="aop.matcher" values=""/>
	
	<generate-with class="com.gwtent.gen.aop.AOPProxyGenerator">
		<when-type-assignable class="com.gwtent.client.aop.Aspectable"/>
	</generate-with>

    <source path="client" />
</module>